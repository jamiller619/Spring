{
  "manifest_version": 2,
  "name": "Spring",
  "description": "A simple, new tab page for Google Chrome.",
  "version": "0.1",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "assets/images/icon.png"
  },
  "icons": {
    "128": "assets/images/icon_128.png"
  }
}
