{
  "name": "Spring",
  "description": "A minimal new tab page for Google Chrome.",
  "version": "0.7.0",
  "manifest_version": 2,
  "incognito": "split",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_title": "Spring",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "images/icon128.png"
  }
}
